<table>
  <thead>
    <tr>
      <td>First Name</td>
      <td>Last Name</td>
      <td>Email</td>
    </tr>
  </thead>
  <tbody>
    <% @students.each do |student| %>
    <tr>
      <td><%= student.fname %></td>
      <td><%= student.lname %></td>
      <td><%= student.email %></td>
      <td><a href="<%= student_path @dojo.id, student.id %>">Show</a> | <a href="<%= edit_student_path @dojo.id, student.id %>">Edit</a> | <a href="<%= destroy_student_path @dojo.id, student.id %>" data-method="delete">Destroy</a></td>
    </tr>
    <% end %>
  </tbody>
</table>

<a href="<%= new_student_path @dojo.id %>">New Student</a>
<a href="<%= dojo_path @dojo.id %>">Go Back</a>
